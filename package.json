{
    "name": "clurry",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@emotion/react": "^11.6.0",
        "@emotion/styled": "^11.6.0",
        "@mui/icons-material": "^5.1.0",
        "@mui/material": "^5.1.0",
        "@mui/styles": "^5.1.0",
        "date-fns": "^2.22.1",
        "firebase": "^9.4.1",
        "markdown-it": "^12.2.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-router-dom": "^5.2.0",
        "react-scripts": "^4.0.3"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "lint": "eslint src/*",
        "lint:fix": "eslint --fix src/*"
    },
    "eslintConfig": {
        "extends": [
            "eslint:recommended",
            "react-app"
        ],
        "rules": {
            "semi": [
                "warn",
                "never"
            ],
            "arrow-parens": "warn",
            "arrow-spacing": [
                "warn",
                {
                    "before": true,
                    "after": true
                }
            ],
            "no-trailing-spaces": "error",
            "jsx-quotes": [
                "warn",
                "prefer-single"
            ],
            "quotes": [
                "warn",
                "single"
            ],
            "object-curly-spacing": [
                "warn",
                "always"
            ]
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@types/markdown-it": "^12.2.3",
        "@types/node": "^15.12.5",
        "@types/react": "^17.0.11",
        "@types/react-dom": "^17.0.8",
        "@types/react-router-dom": "^5.1.7",
        "eslint": "^7.32.0",
        "typescript": "^4.3.5"
    }
}
